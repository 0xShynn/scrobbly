{
  "expo": {
    "name": "Scrobbly",
    "slug": "scrobbly",
    "version": "1.0.2",
    "description": "Scrobbly is a music data reader (iOS app) for your Last.fm account.",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#111111"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": false,
      "bundleIdentifier": "com.scrobbly.mobileApp",
      "buildNumber": "1.0.2"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
